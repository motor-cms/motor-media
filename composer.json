{
  "name": "motor-cms/motor-media",
  "description": "Motor media module",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^8.3",
    "motor-cms/motor-admin": "^3.0",
    "php-ffmpeg/php-ffmpeg": "^v1.3",
    "league/flysystem-aws-s3-v3": "^3.29",
    "spatie/laravel-medialibrary": "^11.11"
  },
  "require-dev": {
    "pestphp/pest": "^3.7",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Motor\\Media\\": "src",
      "Tests\\Motor\\Media\\": "tests",
      "Motor\\Media\\Database\\Factories\\": "database/factories/",
      "Motor\\Media\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\Media\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
